{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "sherlock-registry",
  "homepage": "https://github.com/SplashCodeDex/sherlock",
  "items": [
    {
      "name": "hello-world",
      "type": "registry:component",
      "title": "Hello World",
      "description": "A simple hello world component",
      "registryDependencies": ["button"],
      "files": [
        {
          "path": "registry/new-york/blocks/hello-world/hello-world.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "sherlock-dashboard",
      "type": "registry:block",
      "title": "Sherlock Dashboard",
      "description": "A comprehensive security analysis dashboard with search functionality, results display, and activity monitoring.",
      "registryDependencies": [
        "card",
        "badge",
        "button",
        "input",
        "tabs",
        "progress",
        "alert",
        "table",
        "scroll-area"
      ],
      "files": [
        {
          "path": "registry/new-york/blocks/sherlock-dashboard/page.tsx",
          "type": "registry:page",
          "target": "app/dashboard/page.tsx"
        },
        {
          "path": "registry/new-york/blocks/sherlock-dashboard/components/dashboard-stats.tsx",
          "type": "registry:component"
        },
        {
          "path": "registry/new-york/blocks/sherlock-dashboard/components/search-results.tsx",
          "type": "registry:component"
        },
        {
          "path": "registry/new-york/blocks/sherlock-dashboard/components/recent-activity.tsx",
          "type": "registry:component"
        },
        {
          "path": "registry/new-york/blocks/sherlock-dashboard/hooks/use-sherlock-data.ts",
          "type": "registry:hook"
        },
        {
          "path": "registry/new-york/blocks/sherlock-dashboard/lib/format-date.ts",
          "type": "registry:lib"
        }
      ],
      "categories": ["dashboard", "security", "analysis"]
    }
  ]
}
